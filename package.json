{
  "name": "warp-cache-node",
  "version": "1.0.0",
  "description": "A cache node for Warp Contracts",
  "author": "just_ppe",
  "license": "MIT",
  "scripts": {
    "start:prod": "docker-compose up -d && node src/listener.js",
