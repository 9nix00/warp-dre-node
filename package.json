    "jsonpath-plus": "^7.2.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "4.3.0",
    "koa-compress": "^5.1.0",
    "warp-contracts": "1.2.17",
    "warp-contracts-lmdb": "1.1.1",
    "warp-contracts-pubsub": "^1.0.3"
  },
  "devDependencies": {
    "deep-object-diff": "^1.1.9",
    "ws": "^8.11.0"
